// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int t1,t2,nextterm,i;
    t1=0;
    t2=1;
    int N=10;
    printf("fibonacci series upto %d terms:\n",N);
    printf("%d%d",t1,t2);
    i=3;
    while (i<=N, i++){
        nextterm=t1+t2;
        t1=t2;
        t2=nextterm;
        printf(" ,%d",nextterm);
    }
    printf("\n");
    return 0;
}
